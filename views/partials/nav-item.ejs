<span><a href="/documentation/<%= navItem.slug %>"><%= navItem.displayName %></a></span>

<% if(navItem.children && navItem.children.length) { %>
<ul>
  <% _.each(navItem.children, function (childPath){
    var childData = _.find(data.templateList, {path: childPath});
  %>

  <li>
  <%- partial('../partials/nav-item', {navItem: childData}) %>
  </li>

  <% }); %>
</ul>
<% } %>
